{
  "name": "plaisio/page-core",
  "description": "PhpPlaisio: Core Page",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Page"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio/babel": "^2.1.0",
    "plaisio/company-resolver": "^2.1.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/page": "^1.3.0",
    "plaisio/session": "^5.0.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "plaisio/console-kernel": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Page\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
