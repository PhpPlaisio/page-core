{
  "name": "plaisio/page-core",
  "description": "PhpPlaisio: Core Page",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Page"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/babel": "^2.0",
    "plaisio/company-resolver": "^2.0",
    "plaisio/kernel": "^3.1",
    "plaisio/page": "^1.0",
    "plaisio/request": "^0.10.3",
    "plaisio/session": "^3.0"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0",
    "plaisio/console": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Page\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Page\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
